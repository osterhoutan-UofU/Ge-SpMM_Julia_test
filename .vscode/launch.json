    "version": "0.2.0",
    "configurations": [
        {
            "type": "julia",
            "request": "launch",
            "name": "[Julia] Active File",
            "program": "${file}",
            "stopOnEntry": false,
            "cwd": "${workspaceFolder}",
            "juliaEnv": "${command:activeJuliaEnvironment}"
        },
        {
            "type": "julia",
            "request": "launch",
            "name": "[Julia] `Ge-SpMM_test.jl` tests",
            "program": "${workspaceFolder}/Ge-SpMM_test.jl",
            "stopOnEntry": true
        },
        {
            "type": "julia",
            "request": "launch",
            "name": "[Julia] `dbg.jl` File",
            "program": "${workspaceFolder}/dbg.jl",
            "stopOnEntry": false,
            "cwd": "${workspaceFolder}",
            "juliaEnv": "${command:activeJuliaEnvironment}"
        },
    ]
}